<div class="user_image">
  <%= link_to favorites_user_path(t.user.id) do %>
  <%=  attachment_image_tag t.user, :image, :fill, 50, 50, format: 'jpg', fallback: 'mv.png', :size => '50x50'  %>
</div>
<div class="user_name">
  <%= t.user.name %>
  <% end %>
  <div><%= date_format(t.created_at) %></div>
</div>
